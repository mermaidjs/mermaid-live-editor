    "enzyme-adapter-react-16": "^1.7.1",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.11.0",
    "standard": "^12.0.1",
    "yarn-upgrade-all": "^0.5.0"
